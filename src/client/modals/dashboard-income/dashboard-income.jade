.modal.dashboard-income
  .row
    h1 {{data.di.name}}
      span(ng-show='data.month.name') , {{data.month.name}}
    input(type='text', ng-model='search', placeholder='Search...')
  table.table.table-responsive
    tbody(ng-repeat='property in list = (data.list | filter:{displayAddress:search}) | orderBy:"offer.Property.Address.Street" | limitTo:limit:limit*(page-1)')
      td
        a.address(href='', ng-click='go(property)') {{property.displayAddress}}
      td.squish(ng-show='data.di.sumtype==="Income"') &pound;{{property.role.Commission}}
  pagination.pagination-sm.pagination(total='list.length', ng-model='page', page-size='limit', page-change='pageChange', show-first-last='true')
  a.close-reveal-modal(ng-click='cancel()') &times;