.mobile-menu(ng-click='mobileMenuOut=false')
  nav(ng-show='auth.getUser()')
    ul.menu
      li(ng-class="{selected:state('dashboard')}")
        a(href='/')
          span Dashboard
      li(ng-class="{selected:state(['cases', 'case'])}")
        a(ui-sref='cases')
          span Cases
      li(ng-class="{selected:state('setup')}", ng-show="auth.checkRoles(['admin', 'superadmin'])")
        a(ui-sref='setup')
          span Setup
      li
        a(href='/api/logout', target='_self') 
          img.logo(src="public/img/icons/logout.svg")
          |  Logout